Algoritmo "Terrenos"
      Var area, comprimento,largura : real

Inicio
      Escreva("Informe o comprimento do terreno: ")
      Leia(comprimento)
      Escreva("Informe a largura do terreno: ")
      Leia(largura)
      area <- comprimento * largura
      Escreva(" A área dp terreno é:", area)
Fimalgoritmo